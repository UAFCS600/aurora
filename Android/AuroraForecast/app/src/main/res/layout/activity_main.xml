<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
	android:layout_height="match_parent" android:paddingBottom="@dimen/activity_vertical_margin"
	android:paddingLeft="@dimen/activity_horizontal_margin"
	android:paddingRight="@dimen/activity_horizontal_margin"
	android:paddingTop="@dimen/activity_vertical_margin" tools:context=".MainActivity">
	<Button android:id="@+id/apply_button" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_alignParentEnd="true"
		android:layout_alignParentLeft="true" android:layout_alignParentRight="true"
		android:layout_alignParentStart="true" android:clickable="false"
		android:enabled="true" android:onClick="setContentView" android:text="Settings" />
	<Button android:id="@+id/about_button" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_alignEnd="@+id/apply_button"
		android:layout_alignParentLeft="true" android:layout_alignParentStart="true"
		android:layout_alignRight="@+id/apply_button" android:layout_below="@+id/apply_button"
		android:enabled="true" android:text="About" />
	<Button android:id="@+id/website_button" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_alignEnd="@+id/about_button"
		android:layout_alignParentLeft="true" android:layout_alignParentStart="true"
		android:layout_alignRight="@+id/about_button" android:layout_below="@+id/about_button"
		android:enabled="true" android:text="Website" />
</RelativeLayout>
